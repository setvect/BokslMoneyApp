{
  "vetur.format.options.useTabs": false,
  "editor.tabSize": 2,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      {"autoFix": true, "language": "vue"}
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
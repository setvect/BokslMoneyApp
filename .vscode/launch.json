{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch and Debug",
    "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron-webpack",
    "runtimeArgs": [
      "dev"
    ],
    "autoAttachChildProcesses": true,
    "internalConsoleOptions": "openOnFirstSessionStart",
    "console": "integratedTerminal"
  }]
}